{
  "name": "phaser-react-tools",
  "version": "1.0.4",
  "description": "Toolkit for integrating Phaser and React.",
  "author": "benrosen",
  "license": "MIT",
  "repository": "benrosen/phaser-react-tools",
  "main": "./lib/cjs/index.js",
  "types": "./lib/esm/index.d.ts",
  "module": "./lib/esm/index.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "deploy": "run-s publish & git push -u origin HEAD",
    "document": "node_modules/.bin/jsdoc -c .jsdoc.json",
    "patch": "npm version patch -m 'Patch %s'",
    "predeploy": "run-s build document",
    "prepublish": "tsc",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "deps:check": "npm-check --skip-unused",
    "deps:update": "npm-check --update --skip-unused --save-exact"
  },
  "peerDependencies": {
    "phaser": "^3.0.0",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-standard": "5.0.0",
    "jsdoc": "4.0.2",
    "microbundle-crl": "^0.13.10",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "phaser": "3.70.0",
    "prettier": "3.2.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "5.3.3"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/node": "20.11.6",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9"
  }
}
